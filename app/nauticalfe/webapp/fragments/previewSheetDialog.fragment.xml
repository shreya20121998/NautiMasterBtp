<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog
        id="previewDialog"
        title="Preview Data"
        stretch="true"
        verticalScrolling="true"
        contentWidth="auto"
        contentHeight="auto"
        beginButton="{closeButton}">
 
        <!-- Table -->
        <Table id="previewTable" mode="MultiSelect" showSeparators="All">
            <!-- Columns -->
            <columns >
                <Column width="120px" hAlign="Begin" vAlign="Middle" >
                    <Label text="Country ID"/>
                </Column>
                <Column width="120px" hAlign="Begin" vAlign="Middle">
                    <Label text="Port Code"/>
                </Column>
                <Column width="120px" hAlign="Begin" vAlign="Middle">
                    <Label text="Port Name"/>
                </Column>
                <Column width="120px" hAlign="Begin" vAlign="Middle">
                    <Label text="Reancho"/>
                </Column>
                <Column width="120px" hAlign="Begin" vAlign="Middle">
                    <Label text="Latitude"/>
                </Column>
                <Column width="120px" hAlign="Begin" vAlign="Middle">
                    <Label text="Longitude"/>
                </Column>
                <Column width="120px" hAlign="Begin" vAlign="Middle">
                    <Label text="CountryName"/>
                </Column>
                <Column width="120px" hAlign="Begin" vAlign="Middle">
                    <Label text="LocID"/>
                </Column>
                <Column width="120px" hAlign="Begin" vAlign="Middle">
                    <Label text="IND"/>
                </Column>
            </columns>
            <!-- Items -->
            <items>
                <ColumnListItem>
                    <cells>
                        <Text text="{CountryID}"/>
                        <Text text="{PortCode}"/>
                        <Text text="{PortName}"/>
                        <Text text="{Reancho}"/>
                        <Text text="{Latitude}"/>
                        <Text text="{Longitude}"/>
                        <Text text="{CountryName}"/>
                        <Text text="{LocID}"/>
                        <Text text="{IND}"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <beginButton>
            <Button press="onPortLocPreviewDialogClose" text="Close"  type="Back"></Button>
        </beginButton>
    </Dialog>
</core:FragmentDefinition>
 